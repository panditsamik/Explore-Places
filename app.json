{
  "expo": {
    "name": "Explore Places",
    "slug": "explore-place",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/location-pin.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "description": "A location-based App with Google Maps and Places API",
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.panditsamik.findplace"
    },
    "android": {
      "package": "com.panditsamik.findplace",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow EXPLORE PLACE to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7ca97b40-c78a-4a73-b0b8-5996d2e8fc18"
      }
    },
    "owner": "panditsamik"
  }
}
